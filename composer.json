{
    "name": "dhii/simple-test",
    "description": "A simple unit testing framework, for inclusion with other tools.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php":  "^5.3.3",
        "dhii/stats-abstract": "dev-master",
        "dhii/collections-abstract": "dev-master"
    },
    "require-dev": {
        "dhii/php-cs-fixer-config": "dev-php-5.3"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\SimpleTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dhii\\SimpleTest\\FuncTest\\": "test/functional/"
        }
    },
    "repositories": [
        {
            "url": "https://github.com/Dhii/php-cs-fixer-config.git",
            "type": "git"
        },
        {
            "url": "https://github.com/Dhii/collections-interface.git",
            "type": "git"
        },
        {
            "url": "https://github.com/Dhii/collections-abstract-base.git",
            "type": "git"
        },
        {
            "url": "https://github.com/Dhii/stats-interface.git",
            "type": "git"
        },
        {
            "url": "https://github.com/Dhii/stats-abstract.git",
            "type": "git"
        },
        {
            "url": "https://github.com/Dhii/collections-abstract.git",
            "type": "git"
        }
    ]
}
